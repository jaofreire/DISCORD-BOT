name: dockercompose15919030067351287631
services:
  botplayer-tauz2.0:
    build:
      context: E:\estudo C#\Bot PlayerTauz-Repository 3.0\DISCORD-BOT\Bot PLayer-Tauz 2.0
      dockerfile: Bot-PLayer-Tauz2.0/Dockerfile
    container_name: bot-container
    depends_on:
      mongo:
        condition: service_started
        required: true
    environment:
      MongoConnectionString: mongodb://jaofreire:Freire@mongo:27017/
      State: production
    image: jaofreire/bot-playertauz-image:latest
    networks:
      net-bot: null
  mongo:
    container_name: mongo-container
    environment:
      MONGO_INITDB_ROOT_PASSWORD: Freire
      MONGO_INITDB_ROOT_USERNAME: jaofreire
    image: mongo:latest
    networks:
      net-bot: null
    ports:
      - mode: ingress
        target: 27017
        published: "27017"
        protocol: tcp
    restart: always
  mongo-express:
    container_name: mongoexpress-container
    environment:
      ME_CONFIG_BASICAUTH: "false"
      ME_CONFIG_MONGODB_ADMINPASSWORD: Freire
      ME_CONFIG_MONGODB_ADMINUSERNAME: jaofreire
      ME_CONFIG_MONGODB_URL: mongodb://jaofreire:Freire@mongo:27017/
    image: mongo-express
    networks:
      net-bot: null
    ports:
      - mode: ingress
        target: 8081
        published: "8081"
        protocol: tcp
    restart: always
networks:
  net-bot:
    name: dockercompose15919030067351287631_net-bot
    driver: bridge