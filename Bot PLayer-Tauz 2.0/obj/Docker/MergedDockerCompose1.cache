name: dockercompose15919030067351287631
services:
  botplayer-tauz2.0:
    container_name: bot-container
    depends_on:
      mongo:
        condition: service_started
        required: true
      redis:
        condition: service_started
        required: true
    environment:
      REDIS_CONNECTION_STRINGS: redis-container
    image: jaofreire/bot-playertauz-image:latest
    networks:
      net-bot: null
  mongo:
    container_name: mongo-container
    environment:
      MONGO_INITDB_ROOT_PASSWORD: "128309128309128"
      MONGO_INITDB_ROOT_USERNAME: jaofreire
    image: mongo:latest
    networks:
      net-bot: null
    ports:
      - mode: ingress
        target: 27017
        published: "27017"
        protocol: tcp
    restart: always
  redis:
    container_name: redis-container
    image: redis:latest
    networks:
      net-bot: null
    ports:
      - mode: ingress
        target: 6379
        published: "2010"
        protocol: tcp
    restart: always
networks:
  net-bot:
    name: dockercompose15919030067351287631_net-bot
    driver: bridge